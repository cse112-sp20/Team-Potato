{
  "name": "potato-tab",
  "version": "1.0.0",
  "description": "A tab management chrome extension build by the Potato team.",
  "scripts": {
    "start": "react-scripts start",
    "test": "jest --silent --coverage",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "report": "nyc report"
  },
  "repository": {
