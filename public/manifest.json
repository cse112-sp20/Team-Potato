{
  "manifest_version": 2,
  "key": "",
  "name": "PotatoTab",
  "version": "1.0",
  "description": "An all-in-one tab grouper, session manager, and concentration extension",
  "icons": {
    "16": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "permissions": [
    "activeTab",
    "notifications",
    "storage",
    "tabs",
    "unlimitedStorage",
    "*://*/*",
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
